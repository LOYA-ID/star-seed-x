{
  "source": {
    "host": "localhost",
    "port": 3306,
    "user": "your_source_user",
    "password": "your_source_password",
    "database": "your_source_database",
    "table": "your_source_table",
    "connectionPoolSize": 5
  },
  "destination": {
    "host": "localhost",
    "port": 3306,
    "user": "your_dest_user",
    "password": "your_dest_password",
    "database": "your_dest_database",
    "table": "your_dest_table",
    "connectionPoolSize": 5
  },
  "etl": {
    "batchSize": 1000,
    "primaryKeyColumn": "id",
    "deletedFlagColumn": "is_deleted",
    "sqlQuery": "SELECT * FROM {{table}}",
    "cronSchedule": "*/5 * * * *"
  },
  "sqlite": {
    "dbPath": "./data/etl_state.db"
  },
  "logging": {
    "level": "info",
    "logDir": "./logs"
  }
}
